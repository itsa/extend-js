{
    "name": "js-ext",
    "version": "0.0.1",
    "description": "ITSA's language extensions",
    "author": {
        "name": "Marco Asbreuk",
        "email": "marco@itsasbreuk.nl"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/itsa/js-ext"
    },
    "bugs": {
        "url": "https://github.com/itsa/js-ext/issues"
    },
    "keywords": [
        "ITSA",
        "extend",
        "javascript"
    ],
    "main": "js-ext-base.js",
    "dependencies": {
        "polyfill": "itsa/polyfill",
        "utils": "itsa/utils"
    },
    "devDependencies": {
        "chai": "^1.9.1",
        "istanbul": "^0.3.0",
        "jshint": "^2.5.5",
        "mocha": "^1.21.4"
    },
    "directories": {
        "lib": "./lib",
        "extra": "./extra"
    },
    "engines": {
        "node": "*"
    },
    "scripts": {
        "pretest": "jshint ./js-ext.js",
        "test": "mocha tests/*.js  && istanbul cover $(which _mocha) -- tests/*.js",
        "lint": "jshint ./js-ext.js"
    },
    "testling": {
        "harness": "mocha",
        "files": "tests/*.js",
        "browsers": [
            "ie/6..latest",
            "chrome/22..latest",
            "firefox/16..latest",
            "safari/latest",
            "opera/11.0..latest",
            "iphone/6",
            "ipad/6",
            "android-browser/latest"
        ]
    },
    "homepage": "http://itsa.github.io",
    "maintainers": [
        {
            "name": "Marco Asbreuk",
            "email": "marco@itsasbreuk.nl"
        }
    ],
    "license": "New BSD"

}